<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.T4tip">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">

        <activity android:name=".ui.activities.roomPageActivity" android:screenOrientation="locked"/>
        <activity android:name=".add_brand" android:screenOrientation="locked" />
        <activity android:name=".add_product" android:screenOrientation="locked"/>
        <activity android:name=".ui.activities.FlatDetails" android:screenOrientation="locked"/>
        <activity android:name=".ui.activities.Register" android:screenOrientation="locked"/>
        <activity android:name=".ui.activities.Mainpage" android:screenOrientation="locked"/>
        <activity android:name=".ui.activities.AddFlat" android:screenOrientation="locked"/>
        <activity android:name=".ui.activities.Login" android:screenOrientation="locked">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>